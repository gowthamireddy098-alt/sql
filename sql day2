INSERT INTO students (id, name, age)
VALUES (1, 'kusma', 21),
       (2, 'gowhami', 22);

-- Example with NULL
INSERT INTO students (id, name, age)
VALUES (3, 'Charlie', NULL);

-- Example with DEFAULT (Assuming age has a DEFAULT value)
INSERT INTO students (id, name)
VALUES (4, 'David');

-- UPDATE with WHERE
UPDATE students
SET age = 21
WHERE id = 1;

-- DELETE with WHERE
DELETE FROM students
WHERE id = 2;
